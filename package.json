{
  "name": "cypress-cucumber-example",
  "version": "1.0.0",
  "description": "Example of cypress with cucumber",
  "main": "index.js",
  "scripts": {
    "test": "cypress run --spec \"**/*.feature\"",
    "test:all": "cypress run --spec \"**/*.features\"",
    "cy-test:data": "./node_modules/.bin/cypress-tags run -b chrome -e TAGS='@parallel:data'",
    "cy-test:parallel-node-1": "./node_modules/.bin/cypress-tags run -b chrome -e TAGS='@parallel-1' --env allure=true,allureResultsPath=cypress/allure-results",
    "cy-test:parallel-node-2": "./node_modules/.bin/cypress-tags run -b chrome -e TAGS='@parallel-2' --env allure=true,allureResultsPath=cypress/allure-results",
    "cy-test:parallel-node-3": "./node_modules/.bin/cypress-tags run -b chrome -e TAGS='@parallel-3' --env allure=true,allureResultsPath=cypress/allure-results",
    "cy-test:parallel-node-4": "./node_modules/.bin/cypress-tags run -b chrome -e TAGS='@parallel-4' --env allure=true,allureResultsPath=cypress/allure-results",
    "cy-test:parallel-node-5": "./node_modules/.bin/cypress-tags run -b chrome -e TAGS='@parallel-5' --env allure=true,allureResultsPath=cypress/allure-results",
    "cy-test:parallel-node-6": "./node_modules/.bin/cypress-tags run -b chrome -e TAGS='@parallel-6' --env allure=true,allureResultsPath=cypress/allure-results",
    "cy-test:parallel": "./node_modules/.bin/cypress-tags run -b chrome --headed -e --env envFile=${npm_config_env}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheBrainFamily/cypress-cucumber-example.git"
  },
  "keywords": [
    "cypress",
    "cucumber",
    "testing"
  ],
  "author": "Lukasz Gandecki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheBrainFamily/cypress-cucumber-example/issues"
  },
  "homepage": "https://github.com/TheBrainFamily/cypress-cucumber-example#readme",
  "dependencies": {
    "cypress": "9.3.1",
    "cypress-cucumber-preprocessor": "^2.5.5",
    "cypress-tags": "^0.3.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "cypress/stepsDefinition",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.28.0",
    "allure-commandline": "^2.18.1"
  }
}
